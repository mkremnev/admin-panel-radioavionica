{
  "name": "radioavionica/admin-panel",
  "description": "Project React+Slim",
  "type": "project",
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "require": {
    "php": "^7.4",
    "ext-json": "^7.4",
    "doctrine/migrations": "^3.0",
    "doctrine/orm": "^2.8",
    "laminas/laminas-config-aggregator": "^1.4",
    "php-di/php-di": "^6.3",
    "ramsey/uuid": "^4.1",
    "slim/psr7": "^1.2",
    "slim/slim": "^4.5",
    "symfony/console": "^5.1",
    "webmozart/assert": "^1.9"
  },
  "require-dev": {
    "doctrine/data-fixtures": "^1.4",
    "overtrue/phplint": "^2.2",
    "phpunit/phpunit": "^9.4",
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "*",
    "swiftmailer/swiftmailer": "^6.0",
    "vimeo/psalm": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "license": "GPL",
  "authors": [
    {
      "name": "Maxim Kremnev",
      "email": "m.kremnev@netlinux.ru"
    }
  ],
  "scripts": {
    "lint": "phplint",
    "check": "phpcs",
    "fix": "phpcbf",
    "test": "phpunit --color=always",
    "test-coverage": "phpunit --color=always --coverage-html var/coverage",
    "psalm": "psalm --alter --issues=MissingReturnType,InvalidNullableReturnType",
    "app": "php bin/app.php --ansi --no-interaction"
  }
}
